# GitHub metadata
GITHUB_ORG=__ORG_NAME__
GITHUB_REPO=__REPO_NAME__
GITHUB_PROJECT_NUMBER=

# Token (set locally, never commit real value)
GITHUB_API_TOKEN=

# LLM provider configuration (major hosted providers)
# Supported values: auto, openai, anthropic, gemini
LLM_PROVIDER=__LLM_PROVIDER__
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GEMINI_API_KEY=
# Optional alias used by some Gemini SDKs
GOOGLE_API_KEY=

# Context validation toggles
REQUIRE_AWS_CONTEXT=false
REQUIRE_GRAFANA_CONTEXT=false

# Optional context collection vars
AWS_REGION=us-east-1
GRAFANA_URL=
GRAFANA_API_TOKEN=
PRIMARY_DOMAIN=example.com
API_DOMAIN=api.example.com

# Sonar/quality (optional)
SONAR_TOKEN=

# Deployment/monitoring placeholders
STAGING_BASE_URL=
PRODUCTION_BASE_URL=
